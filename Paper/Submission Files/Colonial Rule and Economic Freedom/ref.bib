@article{Borensztein2001,
abstract = {This paper compares the impact of shocks of U.S. interest rates and emerging market bond spreads on domestic interest rates and exchange rates across several emerging market economies with different exchange rate regimes. Consistent with conventional prioris, the results indicate that interest rates in Hong Kong react much more to U.S. interest rate shocks and shocks to international risk premia than interest rates in Singapore. The results are less clear cut in the comparison of Argentina and Mexico: while interes rates (and the exchange rate) in Mexico seem to react less to U.S. interest rate shocks, they react about the same to bond spread shocks, in addition to a significant impact on the exchange rate.},
author = {Borensztein, Eduardo and Zettelmeyer, Jeromin and Philippon, Thomas},
institution = {IMF},
journal = {IMF Working Paper},
keywords = {exchange rate regimes,interest rates,monetary policy,risk premium},
series = {IMF Working Paper},
title = {{Monetary Independence in Emerging Markest: Does the Fx Regime Make a Difference?}},
volume = {01/1},
year = {2001}
}
@unpublished{Borio2012,
abstract = {It is high time we rediscovered the role of the financial cycle in macroeconomics. In the environment that has prevailed for at least three decades now, it is not possible to understand business fluctuations and the corresponding analytical and policy challenges without understanding the financial cycle. This calls for a rethink of modelling strategies and for significant adjustments to macroeconomic policies. This essay highlights the stylised empirical features of the financial cycle, conjectures as to what it may take to model it satisfactorily, and considers its policy implications. In the discussion of policy, the essay pays special attention to the bust phase, which is less well explored and raises much more controversial issues.},
author = {Borio, Claudio},
institution = {Bank for International Settlements},
keywords = {balance sheet,balance sheet recessions,business cycle,crises,financial,financial cycle,medium term,monetary economy},
number = {395},
series = {BIS Working Papers},
title = {{The Financial Cycle and Macroeconomics: What Have We Learnt?}},
url = {http://www.bis.org/publ/work395.htm},
year = {2012}
}
