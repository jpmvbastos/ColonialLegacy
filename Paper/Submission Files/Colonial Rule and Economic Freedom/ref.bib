@inproceedings{Gomez2006,
abstract = {El trabajo tiene como objetivo determinar la participaci{\'{o}}n del Banco Provincial de; C{\'{o}}rdoba; uno de los dos bancos m{\'{a}}s importantes del interior del pa{\'{i}}s; en la crisis bancaria de 1890/91. En particular; propone establecer si este Banco corri{\'{o}} la misma suerte en la crisis que los bancos l{\'{i}}deres del sistema –el Nacional y el de la Provincia de Buenos Aires-.; Los resultados obtenidos revelan que el desarrollo de la crisis tuvo en el Banco de; C{\'{o}}rdoba una din{\'{a}}mica distinta a la observada en los bancos Nacional y de la Provincia de Buenos Aires. A diferencia de lo ocurrido en estas entidades; la primera corrida bancaria de comienzos de 1890 no se registr{\'{o}} en el Banco de C{\'{o}}rdoba. La segunda corrida; en contraste con lo acontecido en los bancos l{\'{i}}deres; fue devastadora para el Banco de C{\'{o}}rdoba. Entre mayo y junio de 1890; {\'{e}}ste perdi{\'{o}} el 79% de sus dep{\'{o}}sitos; con reservas l{\'{i}}quidas que apenas representaban el 34% de los mismos. Para septiembre; el gobierno nacional orden{\'{o}} su suspensi{\'{o}}n; al mismo tiempo que le conced{\'{i}}a redescuentos; por intermedio del Banco Nacional; para que pudiera regularizar su situaci{\'{o}}n. Empero; la actitud del gobierno cambi{\'{o}} en los primeros meses de 1891; cuando le solicit{\'{o}} al Banco de C{\'{o}}rdoba el pago de sus abultadas deudas en oro. Esto signific{\'{o}} para el Banco la imposibilidad de reabrir sus puertas. Simult{\'{a}}neamente; el gobierno tambi{\'{e}}n dej{\'{o}} caer a los bancos Nacional y de la Provincia de Buenos Aires; en circunstancia de la tercera corrida de abril de 1891.},
address = {Salta},
author = {G{\'{o}}mez, M{\'{o}}nica and Giuliano, Fernando},
booktitle = {Anales},
publisher = {Asociacion Argentina de Economia Politica},
title = {{El Banco Provincial de C{\'{o}}rdoba en la Crisis Bancaria de 1890/91}},
year = {2006}
