@article{Kirzner2010,
abstract = {Klein and Briggeman's condemnatory judgments (a) of this author and (b) of modern Austrian economics are interpreted as apparently attributable to a pre-analytical rejection of all purely economic normative discussion. Upon examination, all their specific criticisms readily dissolve as representing unfortunate misunderstandings or argumentation inappropriately applying their worldview (in which there is no normative role for the purely economic) to a quite different conceptual context. Klein and Briggeman's revisionis doctrinal classification (resenting modern Austrian economics as contrasted(!) with Hayek's work) is categorically rejected as reflecting (i) quite mistaken assumptions concerning modern Austrian economics and (ii) serious misunderstandings of Hayek's contributions.},
