title = {{The calculus of piratical consent: the myth of the myth of social contract}},
volume = {139},
year = {2009}
}
@article{Leeson2007b,
abstract = {Could anarchy be good for Somalia's development? If state predation goes unchecked government may not only fail to add to social welfare, but can actually reduce welfare below its level under statelessness. Such was the case with Somalia's government, which did more harm to its citizens than good. The gov- ernment's collapse and subsequent emergence of statelessness opened the opportunity for Somali progress. This paper investigates the impact of anarchy on Somali development. The data suggest that while the state of this development remains low, on nearly all of 18 key indicators that allow pre- and post-stateless welfare comparisons, Somalis are better off under anarchy than they were under government. Renewed vibrancy in critical sectors of Somalia's economy and public goods in the absence of a predatory state are responsible for this improvement.},
author = {Leeson, Peter T.},
doi = {10.1016/j.jce.2007.10.001},
issn = {01475967},
journal = {Journal of Comparative Economics},
keywords = {Anarchy,Predation,Somalia},
month = {dec},
number = {4},
pages = {689--710},
title = {{Better off stateless: Somalia before and after government collapse}},
volume = {35},
year = {2007}
}
