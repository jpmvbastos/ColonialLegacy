doi = {10.1515/jbvela-2016-0018},
issn = {2194-5861},
journal = {Journal of Business Valuation and Economic Loss Analysis},
month = {jan},
number = {s1},
title = {{Capital Valuation, What is it and Why does it Matter? Insights from Austrian Capital Theory}},
url = {http://www.degruyter.com/view/j/jbvela.2017.12.issue-s1/jbvela-2016-0018/jbvela-2016-0018.xml},
volume = {12},
year = {2017}
}
