@article{White1999a,
abstract = {Hayek's critique of price-level stabilizatin was based on the claim that only a constant money stock, (M), or constant volume of nominal spending, (MV), allows intertemporal price equilibrium. The claim is not generally correct. Hayek's case (in principle) for constant MV and his critique of the automatic gold standard for not delivering it are thus uncompelling. The injection effects of his business cycle theory provided an alternative basis for his prescription. In the 1970s Hayek switched to endorsing price-level stabilitization. On doing so he was logically compelled to repudiate his business cycle theory.},
author = {White, Lawrence H.},
journal = {Journal of Money, Credit and Banking},
number = {1},
pages = {109--120},
title = {{Hayek's Monetary Theory and Policy: A Critical Reconstruction}},
volume = {31},
year = {1999}
}
@article{White2008c,
abstract = {The standard allocative efficiency criteria used by economists (Pareto efficiency and Kaldor-Hicks effciency) are fundamentally unable to rank a slave-labor system against a free-labor system. Given either set of initial property rights assignments the market can reach (or fail to reach) allocative efficiency (that is, allocate resources to their highest-valued uses), but welfare economics provides no meta-framework for ranking initial assignments. This finding underscores the limits to the usefulness of efficiency criteria: they cannot settle all questions, and unfortunately are least decisive just where the stakes are greatest (namely, where wealth effects of alternative assignments are greatest). Excplicitly ethical criteria are needed to make the ranking.},
author = {White, Lawrence H.},
doi = {10.1177/1470594X08092106},
issn = {1470-594X},
journal = {Politics, Philosophy & Economics},
keywords = {Kaldor-Hicks efficiency,Pareto efficiency,property rights,welfare economics},
month = {aug},
number = {3},
pages = {327--340},
title = {{Can economics rank slavery against free labor in terms of efficiency?}},
volume = {7},
year = {2008}
}
